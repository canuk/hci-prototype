<p id="notice"><%= notice %></p>
<br/>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Edit Prompt</h3>
  </div>
  <div class="panel-body">
<p>
  <strong>Text:</strong>
  <%= @prompt.prompt_text %>
</p>

<p>
  <strong>Prompt type:</strong>
  <%= @prompt.prompt_type %>
</p>

<p>
  <strong>Active:</strong>
  <%= @prompt.active %>
</p>
  </div>
  <div class="panel-footer">
    <%= link_to 'Edit', edit_prompt_path(@prompt), class: "btn btn-info" %>
    <%= link_to 'Back', prompts_path, class: "btn btn-warning" %>
    </div>
</div>
